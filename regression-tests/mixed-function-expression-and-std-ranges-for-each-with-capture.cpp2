#include <vector>
#include <ranges>
#include <string>
#include <span>
#include <algorithm>
#include <iostream>

main: () -> int = {
    vec: std::vector<std::string>
            = ("hello", "2022");
    view: std::span = vec;

    y := "\n";
    std::ranges::for_each
        ( view, :(x) = std::cout << x << y$ );

    callback := :(inout x) = x += "-ish";
    std::ranges::for_each( view, callback );

    for view do (str)
        std::cout << str << "\n";
}
